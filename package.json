{
  "name": "ppo-tfjs",
  "version": "0.0.1",
  "description": "Proximal Policy Optimization (PPO) in Tensorflow.js",
  "keywords": [
    "reinforcement learning",
    "ppo",
    "tensorflow.js",
    "tfjs"
  ],
  "main": "dist/index.js",
  "type": "module",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "test": "tsc --build && node test.js",
    "watch": "jest --watchAll"
  },
  "author": "Anton Zemlyansky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zemlyansky/ppo-tfjs.git"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.13.0"
  },
  "devDependencies": {
    "@tensorflow/tfjs-node": "^4.15.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.2.2"
  },
  "jest": {
    "collectCoverage": true,
    "modulePathIgnorePatterns": [
      "temp/",
      "node_modules/"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
